<%- include('/partials/nav') %> 
<section class="main">
    <% searchedMovies.forEach(ele => { %>
    <article>
        <a href="/details/<%= ele.id %>">
            <img src="https://image.tmdb.org/t/p/w500<%= ele.poster_path %> " alt="">
            <h3><%= ele.title%> </h3>
        </a>
    </article>
    <% }) %>
</section>

